{
    "name": "haproxy",
    "version": "0.1.06",
    "slug": "haproxy",
    "arch": ["amd64", "i386", "armhf", "aarch64"],
    "description": "Haproxy server",
    "url": "https://github.com/rbray89/hassio-addons/tree/master/haproxy",
    "startup": "system",
    "boot": "auto",
    "init": false,
    "options": {
	"use_cfg": false,
	"services":[
		{ "type": "main-ssl", "port": 4443, "protocol":"send-proxy-v2" },
		{ "type": "openvpn", "port": 1194, "protocol":"" }
	]
    },
    "ports": {
        "443/tcp": 4443
    },
    "schema": {
	"use_cfg": "bool",    
        "services": [
		{"type": "str", "address":"str", "port": "port", "protocol":"str"}
	]
    },
    "map": ["share:rw"]
}
