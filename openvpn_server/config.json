{
    "name": "OpenVPN Server",
    "version": "0.1.77",
    "slug": "openvpn_server",
    "description": "OpenVPN server for hassio",
    "url": "https://github.com/rbray89/hassio-addons/tree/master/openvpn_server",
    "arch": ["armhf", "aarch64", "amd64", "i386", "armv7"],
    "startup": "system",
    "boot": "auto",
    "init": false,
    "options": {
        "host": "homeassistant.domain.tld",
        "protocol": "udp",
        "remotePort": 1194,
        "ipv6": false,
        "compress": true,
        "clients": ["clientA", "clientB"],
        "server": {
            "ip": "10.8.0.0",
            "mask": "255.255.255.0"
        },
        "routes": [],
        "dns": ["10.8.0.1", "1.1.1.1"]
    },
    "schema": {
        "host": "str",
        "protocol": "match(^(tcp|udp)$)",
        "remotePort": "port",
        "ipv6": "bool",
        "compress": "bool",
        "clients": ["match(^[^ ]+$)"],
        "server": {
        "ip": "str",
        "mask": "str"
        },
        "routes": [
        {
            "ip": "str",
            "mask": "str"
        }
        ],
        "dns": ["str"]
    },
    "ports": {
        "1194/tcp": 1194,
        "1194/udp": 1194
    },
    "map": ["ssl:rw", "share:rw"],
    "privileged": ["NET_ADMIN"],
    "host_network": true
}
